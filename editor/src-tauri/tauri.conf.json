{
    "build": {
        "beforeBuildCommand": "npm run build &amp;&amp; npm run export",
        "beforeDevCommand": "npm run dev",
        "devPath": "http://localhost:3000",
        "distDir": "../out",
        "withGlobalTauri": true
    },
    "package": {
        "productName": "fasing-editor",
        "version": "0.1.0"
    },
    "tauri": {
        "allowlist": {
            "all": false,
            "dialog": {
                "all": false,
                "open": true,
                "save": true
            }
        },
        "bundle": {
            "active": true,
            "category": "DeveloperTool",
            "copyright": "",
            "deb": {
                "depends": []
            },
            "externalBin": [],
            "icon": [
                "icons/32x32.png",
                "icons/128x128.png",
                "icons/128x128@2x.png",
                "icons/icon.icns",
                "icons/icon.ico"
            ],
            "identifier": "com.tauri.dev",
            "longDescription": "",
            "macOS": {
                "entitlements": null,
                "exceptionDomain": "",
                "frameworks": [],
                "providerShortName": null,
                "signingIdentity": null
            },
            "resources": [],
            "shortDescription": "",
            "targets": "all",
            "windows": {
                "certificateThumbprint": null,
                "digestAlgorithm": "sha256",
                "timestampUrl": ""
            }
        },
        "security": {
            "csp": null
        },
        "updater": {
            "active": false
        },
        "windows": [
            {
                "fullscreen": false,
                "height": 800,
                "resizable": true,
                "title": "繁星",
                "width": 1024
            }
        ]
    }
}